  "private": true,
  "type": "module",
  "scripts": {
    "build": "prisma generate && prisma migrate deploy && next build",
    "check": "next lint && tsc --noEmit",
    "db:generate": "prisma migrate dev",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "dev": "next dev --turbo -H 0.0.0.0",
    "dev:docker": "docker compose up -d",
    "dev:docker:stop": "docker compose down",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "postinstall": "prisma generate",
    "prepare": "husky",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "start": "next start",
    "push:prompts": "dotenv -e .env -- pnpm dlx tsx src/core/adapters/llmAdapter/uploadPrompts.ts",
    "eval": "dotenv -e .env -- pnpm dlx tsx src/core/adapters/llmAdapter/assignment/__tests__/evaluateSolution.eval.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
